        <div class="page {{ pageClass }}" ng-view></div>

        <script src="/res/js/jquery-1.10.2.min.js"></script>
        <script src="/res/js/jquery-cookie-1.4.1.js"></script>
        <script src="/res/js/jquery.autosize.js"></script>
