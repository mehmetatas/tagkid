        <script src="/res/js/app/app.js"></script>
        <script src="/res/js/app/signupController.js"></script>
        <script src="/res/js/app/signinController.js"></script>
        <script src="/res/js/app/dashboardController.js"></script>

        <script src="/res/js/app/tg-editor.js"></script>
    </body>
</html>