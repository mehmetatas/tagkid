<div class="hbox hbox-auto-xs hbox-auto-sm">
    <div class="col">
        <div class="padder">
            <div class="streamline b-l b-info m-l m-b padder-v">
                <!-- New Post -->
                <div id="postEdit">
                    <a ui-sref="pages.user({username: user.Username})" class="pull-left thumb-sm avatar m-l-n-md">
                        <img ng-src="{{user.ProfileImageUrl}}" ng-alt="{{user.Fullname}}">
                    </a>
                    <div class="m-l-lg panel b-a r-3x">
                        <div class="panel-heading pos-rlt b-b b-light">
                            <span class="arrow left pull-up"></span>
                            <input type="text" id="tk-title" placeholder="Title here" class="r-t form-control no-border no-padder" style="font-size: 24px" ng-model="newPost.Title" />

                            <div tk-tag-search new-post="newPost"></div>
                        </div>
                        <div class="panel-body">
                            <div text-angular
                                 ta-target-toolbars="toolbar"
                                 placeholder="Write something..."
                                 ng-model="newPost.HtmlContent"></div>

                            <div>
                                <div class="pull-left m-n">
                                    <div text-angular-toolbar
                                         name="toolbar"
                                         ta-toolbar-class="btn-toolbar"
                                         ta-toolbar-group-class="btn-group"
                                         ta-toolbar-button-class="btn btn-default btn-sm"
                                         ta-toolbar-button-active-class="active"
                                         ta-toolbar="[['bold','italics','strikeThrough'],['ul','ol','pre'],['insertLink','insertImageResp','insertVideo']]"></div>
                                </div>
                                <div class="pull-right">
                                    <a href="#" class="btn btn-default btn-sm" ng-click="clear();">
                                        <i class="fa fa-trash"></i> Clear
                                    </a>

                                    <div class="btn-group dropdown" dropdown>
                                        <button class="btn btn-info btn-sm" ng-click="save(1);">
                                            <i class="fa fa-mail-forward text"></i> Post
                                        </button>
                                        <button class="btn btn-info btn-sm" dropdown-toggle><span class="caret"></span></button>
                                        <ul class="dropdown-menu text-xs">
                                            <li><a href ng-click="save(1);"><i class="fa fa-plus"></i> Public</a></li>
                                            <li><a href ng-click="save(0);"><i class="fa fa-minus"></i> Private</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Posts -->
                <tagkid-post ng-repeat="post in posts" on-edit="edit(post)" post="post"></tagkid-post>
            </div>
            <div class="text-center">
                <button class="btn btn-default w-full" ng-click="loadTimeline();" ng-disabled="disableMorePosts" ng-bind="morePostsButtonText"></button>
            </div>
        </div>
    </div>

    <div class="col w-lg lter b-l">
        <div class="vbox">
            <div class="nav-tabs-alt">
                <ul class="nav nav-tabs nav-justified">
                    <li ng-class="{'active': tabs[0]}">
                        <a href ng-click="tab(0)">Day</a>
                    </li>
                    <li ng-class="{'active': tabs[1]}">
                        <a href ng-click="tab(1)">Month</a>
                    </li>
                    <li ng-class="{'active': tabs[2]}">
                        <a href ng-click="tab(2)">Year</a>
                    </li>
                </ul>
            </div>
            <div class="row-row">
